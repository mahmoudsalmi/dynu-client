  "dependencies": {
    "@types/configstore": "^4.0.0",
    "@types/crypto-js": "^3.1.45",
    "@types/inquirer": "^6.5.0",
    "axios": "^0.19.2",
    "chalk": "^4.0.0",
    "commander": "^5.1.0",
    "configstore": "^5.0.1",
    "crypto-js": "^4.0.0",
    "dateformat": "^3.0.3",
    "inquirer": "^7.1.0",
    "public-ip": "^4.0.1"
  },
  "devDependencies": {
    "@types/dateformat": "^3.0.1",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "jest": "^25.5.4",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "tsc": "^1.20150623.0",
    "typescript": "^3.8.3"
