    "test:it": "jest --config jest.it.config.js",
    "test": "npm run test:unit && npm run test:it",
    "coverage": "jest --config jest.config.js --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mahmoudsalmi/dynu-ddns-updater.git"
  },
