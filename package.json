  "description": "update dynu.com DDNS based on public IP given by ipinfo.io",
  "main": "dist/index.js",
  "type": "dist/index.d.js",
  "bin": {
    "@mahmoudsalmi/dynu-ddns-updater": "bin/dynu-ddns-updater",
